{
  "name": "ukiyonovels-client",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "build:static": "yarn build && next export",
    "deploy": "yarn build:static && netlify deploy --dir=out",
    "deploy:prod": "yarn deploy -p",
    "postinstall": "patch-package",
    "relay": "yarn run relay:schema && yarn run relay:compile",
    "relay:schema": "get-graphql-schema http://localhost:5000/graphql > ./schemas/schema.graphql",
    "relay:compile": "relay-compiler --src . --schema ./schemas/schema.graphql --language typescript --artifactDirectory ./schemas/__generated__"
  },
  "devDependencies": {
    "@types/dompurify": "^2.0.2",
    "@types/file-saver": "^2.0.1",
    "@types/node": "^13.13.5",
    "@types/qs": "^6.9.2",
    "@types/react": "^16.9.35",
    "@types/react-relay": "^7.0.8",
    "@types/react-select": "^3.0.12",
    "@types/react-transition-group": "^4.2.4",
    "@types/relay-runtime": "^8.0.10",
    "@types/styled-components": "^5.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-relay": "^9.1.0",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^15.0.0",
    "patch-package": "^6.2.2",
    "postinstall-postinstall": "^2.1.0",
    "react-relay": "^9.1.0",
    "relay-compiler": "^9.1.0",
    "relay-compiler-language-typescript": "^12.0.2",
    "relay-config": "^9.1.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "dompurify": "^2.0.11",
    "dotenv": "^8.2.0",
    "file-saver": "^2.0.2",
    "next": "^9.4.0",
    "polished": "^3.6.3",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-autosize-textarea": "^7.0.0",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-id-swiper": "^3.0.0",
    "react-select": "^3.1.0",
    "react-transition-group": "^4.4.1",
    "relay-runtime": "^9.1.0",
    "simplebar": "^5.2.0",
    "simplebar-react": "^2.2.0",
    "styled-components": "^5.1.0",
    "swiper": "^5.3.8"
  },
  "browser": {
    "fs": false,
    "child_process": false
  }
}
